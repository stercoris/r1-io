{
  "name": "r1-io",
  "version": "1.0.12",
  "description": "Simple jsx based library for creation of vk bots, based on vk-io",
  "main": "dist/index.js",
  "scripts": {
    "test": "yarn test",
    "replacePaths": "tscpaths -p tsconfig.json -s ./src  -o ./dist",
    "transpile": "tsc --project tsconfig.json ",
    "build": "yarn transpile && yarn replacePaths",
    "prepare": "yarn build"
  },
  "author": "Dmitriy Rodin",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9"
  },
  "dependencies": {
    "middleware-io": "^2.8.0",
    "typescript": "^4.4.4",
    "vk-io": "^4.4.0"
  },
  "files": [
    "dist/**/*"
  ]
}
