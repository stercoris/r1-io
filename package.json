{
  "name": "r1-io",
  "version": "1.0.16",
  "description": "Simple jsx based library for creation of vk bots, based on vk-io",
  "main": "dist/index.js",
  "scripts": {
    "lint": "yarn eslint",
    "types": "yarn tsc --project ./tsconfig.json --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test-coverage": "yarn test --coverage",
    "build:transpile": "tsc --project tsconfig.build.json",
    "build:replacePaths": "tsc-alias -p tsconfig.json",
    "build": "yarn build:transpile && yarn build:replacePaths",
    "prepare": "husky install & yarn build"
  },
  "author": "Dmitriy Rodin",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/stercoris/r1-io"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "babel-jest": "^27.4.4",
    "babel-plugin-tsconfig-paths": "^1.0.3-rc.1",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.7.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "middleware-io": "^2.8.0",
    "promise-deep-resolve-ts": "^1.0.5",
    "vk-io": "^4.4.0"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "vk",
    "vk-io",
    "bot",
    "js",
    "node",
    "react",
    "JSX",
    "TSX"
  ]
}
